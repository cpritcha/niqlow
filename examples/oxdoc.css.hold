body {    margin: 32px;    background: white;    font-size: 10pt;    color: black;    font-family: verdana, arial, helvetica, sans-serif;}li {margin-bottom:12px;}table {    font-size: 10pt;    color: black;    font-family: verdana, arial, helvetica, sans-serif;}h1 {    border-bottom: 1px solid black;    font-family: verdana, arial, helvetica, sans-serif;    font-size: 14pt;    font-weight: bold;    padding: 4px;}h2 {    background: #ccccff;    border: 1px solid black;    font-family: verdana, arial, helvetica, sans-serif;    font-size: 12pt;    font-weight: bold;    padding: 4px;    margin-top: 32px;}h3 {    font-size: 14pt;    font-weight: bold;    font-family: times;}dt {    font-weight: bold;    margin-top: 8px;}hr {    border: 0;    width: 100%;     color: silver;    background-color: gray;    height: 1px;    margin-top: 32px;    margin-bottom: 32px;}dd {    margin-left: 24px;}dd.example PRE {    background: #cfd3d3;    border: 1px solid silver;    padding: 4px;}.method_table {    border: 1px solid gray;    font-size: 10pt;    width: 100%;    margin-top: 8px;    border-collapse: collapse;    background: white;}.method_table .header {    text-align: left;    font-size: 11pt;    font-weight: bold;    padding: 4px;    border-bottom: 1px solid gray;}.declaration {    font-family: courier;    font-size: 10pt;}.description {}.method_table .declaration {    border-bottom: 1px solid silver;    border-right: 1px solid silver;    width: 100;    padding: 4px;}.method_table .description {    border-bottom: 1px solid silver;    padding: 4px;}.parameter_table .declaration {    padding-right: 32px;}.equation {    text-align: center;    display: block;    margin: 4px;    font-style: italic;    font-family: times;    font-size: 12pt;}.expression {    font-style: italic;    font-family: times;    font-size: 12pt;}.header {    text-align: right;}.footer {    font-size: 7pt;    text-align: center;    border-top: 1px solid gray;    margin-top: 16px;    padding-top: 4px;}ul.index {    margin-left: 0px;     padding-left: 0px;}.index li {    margin-top: 4px;    list-style-type: none;}.index li a {    text-decoration: none;}span.o { text-decoration : overline }span.n { font-family : courier; font-weight : bolder; color : darkblue; } @media projection{  .page-break  { display:block; page-break-before:always; }    body {    margin: 32px;    background: light-grey;    font-size: 20pt;    color: black;    font-family: verdana, arial, helvetica, sans-serif;    }    h2 {        font-size: 24pt;        height: 36px;        display:block;         page-break-before:always;         padding: 0px;    }    h1 {    text-align:center;    border-bottom: 1px solid #cccccc;    font-family: verdana, arial, helvetica, sans-serif;    font-size: 28pt;    font-weight: bold;    padding: 4px;    height: 40px;    margin-top: 40px;    font-variant: small-caps;    }    dd.example PRE {    font-size : 16pt;    }}